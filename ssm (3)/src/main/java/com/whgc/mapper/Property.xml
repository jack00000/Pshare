<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="com.whgc.mapper.PropertyMapper">
	    <insert id="add" parameterType="Property" >
	        insert into Property_ ( name ) values (#{name})    
	    </insert>
	    
	    <delete id="delete" parameterType="Property" >
	        delete from Property_ where id= #{id}   
	    </delete>
	    
	    <select id="get" parameterType="_int" resultType="Property">
	        select * from   Property_  where id= #{id}    
	    </select>

	    <update id="update" parameterType="Property" >
	        update Property_ set name=#{name} where id=#{id}    
	    </update>
	    <select id="list" resultType="Property">
	        select * from   Property_      
	    </select>
	</mapper>