<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="com.whgc.mapper.PropertyvalueMapper">
	    <insert id="add" parameterType="Propertyvalue" >
	        insert into Propertyvalue_ ( name ) values (#{name})    
	    </insert>
	    
	    <delete id="delete" parameterType="Propertyvalue" >
	        delete from Propertyvalue_ where id= #{id}   
	    </delete>
	    
	    <select id="get" parameterType="_int" resultType="Propertyvalue">
	        select * from   Propertyvalue_  where id= #{id}    
	    </select>

	    <update id="update" parameterType="Propertyvalue" >
	        update Propertyvalue_ set name=#{name} where id=#{id}    
	    </update>
	    <select id="list" resultType="Propertyvalue">
	        select * from   Propertyvalue_      
	    </select>
	</mapper>